{
    "rules": {
        "ban": true,
        "curly": true,
        "eofline": true,
        "forin": true,
        "label-position": true,
        "max-line-length": { "options": [ 120 ] },
        "member-access": true,
        "member-ordering": { "options": [ true ] },
        "new-parens": true,
        "no-any": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": true,
        "no-console": true,
        "no-construct": true,
        "no-debugger": true,
        "no-default-export": true,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-eval": true,
        "no-inferrable-types": true,
        "no-internal-module": true,
        "no-require-imports": true,
        "no-shadowed-variable": true,
        "no-string-literal": true,
        "no-switch-case-fall-through": true,
        "no-unused-expression": true,
        "no-var-keyword": true,
        "no-var-requires": true,
        "radix": true,
        "switch-default": true,
        "triple-equals": true
    },
    "jsRules": {
        "ban": true,
        "eofline": true,
        "forin": true,
        "label-position": true,
        "member-ordering": { "options": [ true ] },
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": true,
        "no-console": true,
        "no-construct": true,
        "no-debugger": true,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-eval": true,
        "no-string-literal": true,
        "no-switch-case-fall-through": true,
        "no-unused-expression": true,
        "radix": true,
        "switch-default": true
    }
}
